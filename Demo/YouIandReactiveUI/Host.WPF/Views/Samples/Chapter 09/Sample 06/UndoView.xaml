<rxui:ReactiveUserControl x:Class="Book.Views.Samples.Chapter09.Sample06.UndoView"
        x:TypeArguments="vms:UndoViewModel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:rxui="http://reactiveui.net"
        xmlns:vms="clr-namespace:Book.ViewModels.Samples.Chapter09.Sample06;assembly=Book.ViewModels"
        xmlns:ma="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:b="clr-namespace:Book.Behaviors"
        Focusable="True">
    <DockPanel
        Background="{StaticResource AccentColorBrush}">
        <Button
            x:Name="undoButton"
            DockPanel.Dock="Right"
            Margin="10"/>
        <Label
            x:Name="confirmDeleteLabel"
            VerticalAlignment="Center"
            FontWeight="Bold"
            FontSize="14"
            Margin="10"/>
    </DockPanel>
</rxui:ReactiveUserControl>