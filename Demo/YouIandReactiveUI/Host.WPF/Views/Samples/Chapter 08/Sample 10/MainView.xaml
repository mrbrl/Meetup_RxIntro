<rxui:ReactiveUserControl x:Class="Book.Views.Samples.Chapter08.Sample10.MainView"
        x:TypeArguments="vms:MainViewModel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:rxui="http://reactiveui.net"
        xmlns:vms="clr-namespace:Book.ViewModels.Samples.Chapter08.Sample10;assembly=Book.ViewModels"
        xmlns:ma="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:b="clr-namespace:Book.Behaviors"
        Focusable="True">
    <Image
        x:Name="image">
        <Image.RenderTransform>
            <ScaleTransform
                x:Name="scaleTransform"/>
        </Image.RenderTransform>
    </Image>
</rxui:ReactiveUserControl>